  </channel>
  <channel id="Discovery Channel FHD [Alter]">
    <display-name lang="pt">Discovery Channel FHD [Alter]</display-name>
    <url>http://www.vivo.com.br</url>
  </channel>
  <channel id="Discovery Channel FHD">
    <display-name lang="pt">Discovery Channel FHD</display-name>
    <url>http://www.vivo.com.br</url>
  </channel>
  <channel id="Discovery Channel FHD H.265">
